{
  "version": 2,
  "env": {},
  "builds": [
    {
      "src": "dist/apps/blog/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/apps/blog/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "continue": true,
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
